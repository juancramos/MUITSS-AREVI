{
  "host": "arevi-api-app.feathersjs.com",
  "port": "PORT",
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "mysql": "MYSQLCONNSTR_localdb",
  "authentication": {
    "secret": "APP_SETTINGS_SECRET_TOKEN",
    "strategies": [
      "jwt",
      "local"
    ],
    "path": "/authentication",
    "service": "user",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "https://yourdomain.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "usernameField": "email",
      "passwordField": "password"
    }
  }
}

